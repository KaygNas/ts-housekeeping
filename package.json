{
  "name": "ts-unused-exports-remove",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@9.1.1",
  "description": "A simple cli to remove unused variables, interface, class in `.ts` files.",
  "author": {
    "name": "Chengjie Huang",
    "email": "597857074@qq.com"
  },
  "homepage": "https://github.com/KaygNas/ts-unused-exports-remove",
  "repository": {
    "type": "git",
    "url": "https://github.com/KaygNas/ts-unused-exports-remove.git"
  },
  "keywords": [
    "dead code",
    "duplicate",
    "export",
    "files",
    "types",
    "typescript",
    "unreferenced",
    "unresolved",
    "unused"
  ],
  "bin": {
    "ts-unused-exports-remove": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "pnpm test",
    "lint": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "prepublish": "pnpm build"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "minimatch": "^9.0.4",
    "ts-morph": "^22.0.0",
    "ts-unused-exports": "^10.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@types/node": "^20.12.12",
    "@unocss/eslint-plugin": "^0.61.0",
    "eslint": "^9.3.0",
    "eslint-plugin-format": "^0.1.1",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.1.1",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.13.8",
    "typescript": "5.5.0-beta",
    "vite": "^5.2.11",
    "vite-plugin-external": "^4.3.1",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
